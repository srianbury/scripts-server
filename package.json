{
  "name": "script-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "SET NODE_ENV=development&&nodemon -r dotenv/config --exec babel-node src/index.js dotenv_config_path=.env.development",
    "start:test": "SET NODE_ENV=test&&nodemon -r dotenv/config --exec babel-node src/index.js dotenv_config_path=.env.test",
    "test": "SET NODE_ENV=test&&mocha -r dotenv/config --require @babel/register 'src/**/*.test.js' dotenv_config_path=.env.test",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel"
  },
  "keywords": [],
  "author": "srianbury",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "chai-exclude": "^2.0.3",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12"
  },
  "dependencies": {
    "@babel/register": "^7.15.3",
    "apollo-server": "^3.1.2",
    "apollo-server-express": "^3.1.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-resolvers": "^0.4.2",
    "graphql-scalars": "^1.10.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5"
  }
}
